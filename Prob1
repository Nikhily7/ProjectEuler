
public class prob1 {

	public static void main(String[] args) {


    Integer arr[]=new Integer[1001];
		int j=1;
		int sum =0;
		arr[0]=0;
		for (int i=1;i<=1000;i++)
		{
			j=i;
			if (j%3==0)
			{
			arr[j]=j;
			}
			else if (j%5==0 && j%3!=0)
			{
				arr[j]=j;
			}
		}
		
		for (int k=0;k<1000;k++)
		{
			if (arr[k]!= null) 
					{
				sum = sum+arr[k];
					}
		}
		
		System.out.println("Sum is "+sum);
	}

}
